#!/usr/bin/env bash

if
  [[ -s "/home/test/.rvm/gems/ruby-2.1.2@global/environment" ]]
then
  source "/home/test/.rvm/gems/ruby-2.1.2@global/environment"
  exec executable-hooks-uninstaller "$@"
else
  echo "ERROR: Missing RVM environment file: '/home/test/.rvm/gems/ruby-2.1.2@global/environment'" >&2
  exit 1
fi
